{
    "preset": "laravel",
    "rules": {
        "class_attributes_separation": {
            "elements": {
                "const": "none",
                "method": "one",
                "property": "none",
                "trait_import": "none",
                "case": "none"
            }
        },
        "not_operator_with_successor_space": false,
        "concat_space": false
    }
}
